    "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naver/egjs-flicking/tree/master/packages/vue3-flicking"
  },
  "author": {
    "name": "NAVER Corp."
  },
  "homepage": "https://naver.github.io/egjs-flicking",
  "bugs": {
    "url": "https://github.com/naver/egjs-flicking/issues"
  },
  "license": "MIT",
  "browserslist": [
    "last 2 version",
    "ie 9-11",
    "not op_mini all"
  ],
  "dependencies": {
    "@egjs/component": "^3.0.2",
    "@egjs/flicking": "~4.11.2"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@egjs/build-helper": "^0.1.2",
    "@egjs/flicking-plugins": "^4.2.2",
    "@rollup/plugin-commonjs": "^19.0.2",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.2.6",
    "cpx": "^1.5.0",
    "rollup-plugin-vue": "^6.0.0",
    "tslib": "^2.3.1",
    "typescript": "4.8.4",
    "vue": "^3.2.22"
  }
}
