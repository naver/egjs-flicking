{
  "name": "e2e",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "e2e": "concurrently -k -r -s \"first\" npm:e2e:open-silent npm:e2e:once",
    "e2e:once": "codeceptjs run --steps",
    "e2e:headless": "HEADLESS=true codeceptjs run --steps",
    "e2e:ui": "concurrently -k -r -s \"first\" npm:e2e:open-silent \"codecept-ui --app\"",
    "e2e:open": "npm run storybook:all",
    "e2e:open-silent": "npm run storybook:all 2>&1 >/dev/null",
    "lint": "eslint 'src/**/*.ts'",
    "storybook": "npm run storybook:all & npm run storybook:main",
    "storybook:main": "node_modules/@storybook/react/bin/index.js -p 6006",
    "storybook:all": "concurrently -r npm:storybook:html npm:storybook:react npm:storybook:vue npm:storybook:vue3 npm:storybook:angular npm:storybook:svelte",
    "storybook:html": "node_modules/@storybook/html/bin/index.js -p 9005 -c .storybook/html -s ./public --quiet",
    "storybook:react": "node_modules/@storybook/react/bin/index.js -p 9006 -c .storybook/react -s ./public --quiet",
    "storybook:vue": "npm --prefix ./run/vue run storybook",
    "storybook:vue3": "npm --prefix ./run/vue3 run storybook",
    "storybook:angular": "node_modules/@storybook/angular/bin/index.js -p 9009 -c .storybook/angular -s ./public --quiet",
    "storybook:svelte": "node_modules/@storybook/svelte/bin/index.js -p 9010 -c .storybook/svelte -s ./public --quiet"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.5",
    "@angular-devkit/core": "^11.2.5",
    "@angular/cli": "^11.2.5",
    "@angular/common": "^11.2.6",
    "@angular/compiler": "^11.2.6",
    "@angular/compiler-cli": "^11.2.6",
    "@angular/core": "^11.2.6",
    "@angular/forms": "^11.2.6",
    "@angular/platform-browser": "^11.2.6",
    "@angular/platform-browser-dynamic": "^11.2.6",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.14.4",
    "@codeceptjs/configure": "^0.6.2",
    "@codeceptjs/examples": "^1.2.1",
    "@codeceptjs/ui": "^0.4.3",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/angular": "^6.1.21",
    "@storybook/client-api": "^6.1.21",
    "@storybook/html": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@storybook/svelte": "^6.3.6",
    "@storybook/vue": "^6.3.6",
    "@storybook/vue3": "^6.3.6",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "codeceptjs": "^3.0.5",
    "codeceptjs-resemblehelper": "^1.9.3",
    "concurrently": "^6.0.0",
    "playwright": "^1.9.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rxjs": "^6.6.6",
    "svelte": "^3.41.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.7.4",
    "ts-node": "^9.1.1",
    "zone.js": "^0.11.4"
  },
  "browserslist": [
    "last 2 version",
    "ie 9-11",
    "not op_mini all"
  ],
  "dependencies": {
    "@egjs/svelte-flicking": "^4.2.2"
  }
}
