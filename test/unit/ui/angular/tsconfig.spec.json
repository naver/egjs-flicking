/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/spec",
    "jsx": "react-jsx",
    "skipLibCheck": true
  },
  "files": [
    "src/test.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "./src/**/*.spec.ts",
    "./src/**/*.spec.tsx",
    "../spec/*.spec.ts",
    "../spec/*.spec.tsx"
  ]
}
