{
  "name": "cfc",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "npm run install:all",
    "install:all": "npm-run-all --parallel install:angular install:react install:vue install:vue3 install:svelte",
    "install:angular": "npm install --prefix framework/angular",
    "install:react": "npm install --prefix framework/react",
    "install:vue": "npm install --prefix framework/vue",
    "install:vue3": "npm install --prefix framework/vue3",
    "install:svelte": "npm install --prefix framework/svelte",
    "test": "npm run test:all",
    "test:all": "npm-run-all --parallel test:vanilla test:angular test:react test:vue test:vue3 test:svelte",
    "test:vanilla": "npm test --prefix framework/vanilla",
    "test:angular": "npm test --prefix framework/angular",
    "test:react": "npm test --prefix framework/react",
    "test:vue": "npm test --prefix framework/vue",
    "test:vue3": "npm test --prefix framework/vue",
    "test:svelte": "npm test --prefix framework/svelte"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.11.1",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "babel-jest": "^27.3.1",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "ts-jest": "^27.0.7",
    "typescript": "~4.2.4"
  }
}
