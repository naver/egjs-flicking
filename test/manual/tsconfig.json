{
  "extends": "../../tsconfig",
  "compilerOptions": {
    "outDir": "./outjs/",
    "module": "es2015",
    "target": "es5",
    "moduleResolution": "node",
    "lib": ["es2015", "dom"],
    "sourceMap": true,
    "strictNullChecks": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "esModuleInterop": false,
    "baseUrl": ".",
    "paths": {
      "~/*": ["../../src/*"]
    }
  },
  "include": [
    "../../src/**/*.ts",
    "./**/*.ts"
  ],
  "exclude": [
    "../node_modules/**/*.ts"
  ]
}
